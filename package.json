{
  "name": "bib-search",
  "displayName": "Bib Search",
  "description": "Quickly populate your bibtex/biblatex references without leaving Visual Studio Code.",
  "repository": {
    "type": "git",
    "url": "https://github.com/PradyumnaShome/google-scholar-bibtex.git"
  },
  "publisher": "pradyumnashome",
  "author": {
    "name": "Pradyumna Shome",
    "email": "pradyumna.shome+googlescholarbibtex@gmail.com"
  },
  "icon": "images/icon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bib-search.search",
        "title": "Bib Search"
      }
    ],
    "configuration": {
      "title": "Bib Search",
      "properties": {
        "bib-search.publicationDatabase": {
          "type": [
            "DBLP",
            "Google Scholar"
          ],
          "default": "DBLP",
          "description": "Which publication database to use for searching. DBLP data is higher quality since we use their API to search, but Google Scholar is more comprehensive, since it indexes venues outside of computer science. Choose `DBLP` for DBLP, and `Google Scholar` for Google Scholar."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "axios": "^1.5.1",
    "cheerio": "^1.0.0",
    "global": "^4.4.0",
    "xml2js": "^0.6.2"
  }
}
